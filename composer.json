{
    "name": "nexus633/laravel-bootstrap-ui",
    "type": "library",
    "description": "Bootstrap 5 UI Components for Livewire",
    "keywords": ["bootstrap","components", "laravel", "livewire"],
    "homepage": "https://github.com/Nexus633/laravel-bootstrap-ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Andre",
            "email": "Nexus633@googlemail.com",
            "role": "Developer"
        }
